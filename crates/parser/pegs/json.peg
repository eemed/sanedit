Value: Object / Array / Number / String / True / False / Null;
Object: "{" Member ("," Member)* "}";
Member: String ":" Value;

Array: "[" Value ("," Value )* "]";

Number: "-"? Integer ("." [0..9]+)?;
String: Quote ([x20..x21] / [x23..x5b] / [x5d..x10ffff])* Quote;
True: "true";
False: "false";
Integer: "0" / [1..9] [0..9]*;
Quote: "\"";
